    {
        "recipeName": "Spicy BBQ Chicken Drumettes",
        "time": {
            "prep": "10",
            "cook": "40"
        },
        "ingredients": [
            [
                "2 pounds chicken drumettes",
                false
            ],
            [
                "½ cup buttermilk (optional)",
                false
            ],
            [
                "6 garlic cloves, minced",
                false
            ],
            [
                "1 teaspoon minced ginger",
                false
            ],
            [
                "2 green onions, chopped",
                false
            ],
            [
                "½ cup Korean hot pepper paste",
                false
            ],
            [
                "1/3 cup rice syrup (or ¼ cup sugar)",
                false
            ],
            [
                "2 tablespoons Korean hot pepper flakes",
                false
            ],
            [
                "1 tablespoon sesame oil",
                false
            ],
            [
                "½ teaspoon ground black pepper",
                false
            ]
        ],
        "directions": [
            "Prep the chicken, if using buttermilk:",
            "Put the chicken in a bowl, add milk and mix it together by hand. Cover and refrigerate for 30 minutes",
            "Make the spicy paste:",
            "Combine garlic, ginger, green onion, hot pepper paste, rice syrup, hot pepper flakes, sesame oil, and black pepper in a bowl and mix it well with a spoon.",
            "If baking: pre-heat oven to 450F",
            "Rinse and dry the chicken with paper towels.  Season with Salt and Pepper.",
            "Add the chicken and spicy paste to a bowl and mix well. Refrigerate until oven is pre-heated.",
            "BBQ chicken:",
            "Grill or BBQ the chicken on medium heat for 30 minutes, or until well cooked. Keep flipping the drumettes over with your tongs so they don't burn on the grill.",
            "If baking: bake the chicken on a rack for 25-30 minutes, flipping half way through",
            "Brush on any remaining hot sauce after the first turn over.",
            "Serve with rice and other side dishes."
        ]
    },
